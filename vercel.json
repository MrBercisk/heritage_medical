{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.heritagemedical.site" }
      ],
      "destination": "https://heritagemedical.site/:1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "heritagemedical.site" },
        { "type": "header", "key": "x-forwarded-proto", "value": "http" }
      ],
      "destination": "https://heritagemedical.site/:1",
      "permanent": true
    }
  ]
}
